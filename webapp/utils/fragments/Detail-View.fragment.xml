<core:FragmentDefinition controllerName="sapui5final.controller.Master-View" displayBlock="true"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    xmlns:semantic="sap.f.semantic"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <FlexBox>
        <VBox width="10rem">
            <ObjectAttribute text="{i18n>productNameText}"/>
            <ObjectAttribute text="{ProductName}"/>
        </VBox>
        <VBox width="10rem">
            <ObjectAttribute text="{i18n>unitPriceText}"/>
            <ObjectAttribute text="{UnitPrice}"/>
        </VBox>
        <VBox width="10rem">
            <ObjectAttribute text="{i18n>unitsInStockText}"/>
            <ObjectAttribute text="{UnitsInStock}"/>
        </VBox>
        <VBox width="10rem">
            <ObjectAttribute text="{i18n>categoryNameText}"/>
            <ObjectAttribute text="{CategoryName}"/>
        </VBox>
        <VBox width="10rem">
            <ObjectAttribute text="{i18n>supplierNameText}"/>
            <ObjectAttribute text="{SupplierName}"/>
        </VBox>
        <VBox width="15rem">
            <ObjectAttribute text="{i18n>discontinued}"/>
            <ObjectAttribute text="{Discontinued}"/>
        </VBox>
        <VBox width="15rem">
            <ObjectAttribute text="{i18n>productNameText}"/>
            <ObjectAttribute text="{ProductName}"/>
        </VBox>
    </FlexBox>
</core:FragmentDefinition>